<?xml version="1.0" encoding="UTF-8"?>
<!-- This file is duplicated in igloo-parent/igloo/igloo-parents/igloo-parent-maven-configuration-common,
	please modify both of the files when you need to modify something-->
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.1.xsd">
	<!-- We are using a Spring Framework version bigger than 5.0.5 so we are not concerned  -->
	<suppress>
		<notes><![CDATA[file name: spring-security-core-5.1.4.RELEASE.jar]]></notes>
		<gav regex="true">^org\.springframework\.security:spring-security-core:.*$</gav>
		<cve>CVE-2018-1258</cve>
	</suppress>

	<suppress>
		<notes><![CDATA[file name: spring-security-acl-5.1.4.RELEASE.jar]]></notes>
		<gav regex="true">^org\.springframework\.security:spring-security-acl:.*$</gav>
		<cve>CVE-2018-1258</cve>
	</suppress>

	<!-- This vulnerability isn't for protobuff but for node's merge-object_project -->
	<suppress>
		<notes><![CDATA[file name: java-object-diff-0.95.jar]]></notes>
		<gav regex="true">^de\.danielbechler:java-object-diff:.*$</gav>
		<cpe>cpe:/a:merge-object_project:merge-object</cpe>
	</suppress>

	<suppress>
		<notes><![CDATA[file name: clipboard-2.0.4.jar]]></notes>
		<gav regex="true">^org\.webjars\.npm:clipboard:.*$</gav>
		<cpe>cpe:/a:clipboard_project:clipboard</cpe>
	</suppress>

	<suppress>
		<notes><![CDATA[file name: delegate-3.2.0.jar]]></notes>
		<gav regex="true">^org\.webjars\.npm:delegate:.*$</gav>
		<cpe>cpe:/a:delegate:delegate</cpe>
	</suppress>
</suppressions>